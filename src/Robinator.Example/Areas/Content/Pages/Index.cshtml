@page
@model Robinator.Core.Areas.RobinatorAdmin.Pages.Content.IndexModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
    ViewData["Title"] = "Types";
}
<header class="masthead" style="background-image: url('/img/about-bg.jpg')">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>Every content</h1>
                    <span class="subheading">See all my types</span>
                </div>
            </div>
        </div>
    </div>
</header>
<ul>
    @foreach (var type in Model.EditTypes)
    {
        <li><a asp-page="./List" asp-route-type="@type.Link">@type.Name</a></li>
    }
</ul>